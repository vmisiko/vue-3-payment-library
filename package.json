    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name @sendyit/pay src/main.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "core-js": "^3.22.4",
    "mitt": "^3.0.0",
    "moment-duration-format": "^2.3.2",
    "moment-timezone": "^0.5.34",
    "moments": "0.0.2",
    "register-service-worker": "^1.7.2",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.14",
    "vue": "^3.2.33",
    "vue3-tel-input": "^1.0.4",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "prettier": "^2.4.1",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0"
  },
  "peerDependencies": {
    "vue-router": "^4.0.0-0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "main": "./dist/@sendyit/pay.common.js",
  "files": [
    "dist/*"
  ],
  "description": "vue plugin library to facilitate payment",
  "keywords": [
    "Sendy Pay",
    "Sendy Payment",
    "Vue plugins",
    "Vue components",
    "Sendyit"
  ],
  "authors": [
    "Victor Misiko <misiko@sendyit.com>",
    "Joyce kemboi <joyce@sendyit.com>"
  ]
